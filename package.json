{
  "name": "shopr",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "rakkas",
    "build": "rakkas build",
    "start": "node dist/server",
    "format": "prettier --write --ignore-unknown src",
    "test": "pnpm test:typecheck && pnpm test:format && pnpm test:lint && pnpm test:unit",
    "test:typecheck": "tsc -p tsconfig.json --noEmit",
    "test:format": "prettier --check --ignore-unknown src",
    "test:lint": "eslint . --ignore-pattern dist",
    "test:unit": "vitest run",
    "prepare": "husky install",
    "codegen:saleor": "graphql-codegen --config adapters/saleor/codegen.yml"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.2.1",
    "@graphql-codegen/introspection": "^3.0.1",
    "@graphql-codegen/typescript": "^3.0.1",
    "@graphql-codegen/typescript-graphql-files-modules": "^2.2.1",
    "@graphql-codegen/typescript-graphql-request": "^4.5.8",
    "@graphql-codegen/typescript-operations": "^3.0.1",
    "@hattip/adapter-netlify-edge": "^0.0.29",
    "@rakkasjs/eslint-config": "0.6.18",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "postcss": "^8.4.21",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^8.0.1",
    "prettier": "2.8.4",
    "rakkasjs": "0.6.18",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5",
    "vite": "^4.1.3",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.28.5"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.6.1",
    "@ant-design/icons": "^5.0.1",
    "@hattip/cookie": "^0.0.29",
    "@tanstack/react-query": "^4.24.10",
    "ahooks": "^3.7.5",
    "antd": "^5.2.3",
    "axios": "^1.3.4",
    "clsx": "^1.2.1",
    "countries-list": "^2.6.1",
    "dayjs": "^1.11.7",
    "editorjs-blocks-react-renderer": "^1.3.0",
    "graphql": "^16.6.0",
    "graphql-request": "^5.2.0",
    "graphql-tag": "^2.12.6",
    "i18next": "^22.4.10",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0"
  },
  "volta": {
    "node": "18.14.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,cjs,mjs}": "eslint --cache --fix",
    "**/*": "prettier --write --ignore-unknown"
  }
}
